    "eslint": "^8.56.0",
    "eslint-config-unjs": "^0.2.1",
    "jiti": "^1.21.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vitest": "^1.1.3"
  },
  "peerDependencies": {
    "@pinia-orm/axios": "^1.7.0",
